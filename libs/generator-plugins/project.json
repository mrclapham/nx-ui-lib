{
  "name": "generator-plugins",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/generator-plugins/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/generator-plugins",
        "main": "libs/generator-plugins/src/index.ts",
        "tsConfig": "libs/generator-plugins/tsconfig.lib.json",
        "assets": [
          "libs/generator-plugins/*.md",
          {
            "input": "./libs/generator-plugins/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./libs/generator-plugins/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./libs/generator-plugins",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./libs/generator-plugins",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/generator-plugins/jest.config.ts"
      }
    }
  },
  "generators": {
    "ui-component-generator": {
      "factory": "./src/generators/ui-component-generator/generator",
      "schema": "./src/generators/ui-component-generator/schema.json"
    }
  }
}
